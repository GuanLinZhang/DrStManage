<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="cn.drst.entity.Permission" table="permission" schema="">
        <id name="permissionId" column="PermissionId"/>
        <property name="permissionName" column="PermissionName"/>
        <property name="permissionExplain" column="PermissionExplain"/>
        <property name="permissionVisible" column="PermissionVisible"/>
        <property name="permissionValue" column="PermissionValue"/>
    </class>
</hibernate-mapping>