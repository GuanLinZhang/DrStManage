<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="cn.drst.entity.Clinic" table="clinic" schema="drst_master">
        <id name="organizationNumber" column="OrganizationNumber"/>
        <property name="clinicName" column="ClinicName"/>
        <property name="clinicState" column="ClinicState"/>
        <property name="clinicAddress" column="ClinicAddress"/>
        <property name="createDate" column="CreateDate"/>
    </class>
</hibernate-mapping>