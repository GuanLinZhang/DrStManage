<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="cn.drst.entity.ArrangeClasses" table="arrangeclasses" schema="drst_master">
        <id name="userId" column="UserId"/>
        <property name="monday" column="Monday"/>
        <property name="tuesday" column="Tuesday"/>
        <property name="wednesday" column="Wednesday"/>
        <property name="thursday" column="Thursday"/>
        <property name="friday" column="Friday"/>
        <property name="saturday" column="Saturday"/>
        <property name="sunday" column="Sunday"/>
    </class>
</hibernate-mapping>